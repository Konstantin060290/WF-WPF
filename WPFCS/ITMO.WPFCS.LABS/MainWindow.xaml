<Window x:Class="ITMO.WPFCS.LABS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.WPFCS.LABS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" Margin="82,48,0,0" VerticalAlignment="Top" Width="123" Height="51" Click="Button_Click"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" Margin="82,121,0,0" VerticalAlignment="Top" Width="123" Height="51" Click="Button_Click_1"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="55" Margin="297,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="233" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Margin="297,121,0,0" VerticalAlignment="Top" Height="51" Width="453"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me"
        Height="23" Name="button3" Width="75" Click="Button3_Click" />
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12"
        Name="myTextBox" />
    </Canvas>
</Window>
